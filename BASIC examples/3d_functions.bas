5 SCREEN 2
10 DIM M(255)
15 A=COS(3.14/4)
20 FOR Y=1 TO 141 STEP 5
25 E=A*Y
30 C=Y-70
35 C=C*C
40 FOR X=1 TO 141
45 D=X-70
50 Z=80*EXP(-0.001*(C+D*D))
55 X1=X+E
60 Y1=Z+E
70 IF Y1>=M(X1) THEN M(X1)=Y1:PLOT X1,191-Y1
80 NEXT X,Y
90 END
